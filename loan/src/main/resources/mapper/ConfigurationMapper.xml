  </resultMap>
  <insert id="insert" parameterType="loan.easyLoan.entity.Configuration">
    insert into configuration (parameter_name, parameter_value)
    values (#{parameterName,jdbcType=VARCHAR}, #{parameterValue,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="loan.easyLoan.entity.Configuration">
    insert into configuration
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="parameterName != null">
        parameter_name,
      </if>
      <if test="parameterValue != null">
        parameter_value,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="parameterName != null">
        #{parameterName,jdbcType=VARCHAR},
      </if>
      <if test="parameterValue != null">
        #{parameterValue,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>